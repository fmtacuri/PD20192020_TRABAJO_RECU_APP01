CREATE TABLE IF NOT EXISTS ADDRESS (
    ID INTEGER NOT NULL PRIMARY KEY,
	USERID INTEGER NOT NULL,
	STREET VARCHAR(60) NOT NULL,
	SUITE VARCHAR(60) NOT NULL,
	CITY VARCHAR(60) NOT NULL,
	ZIPCODE VARCHAR(60) NOT NULL
);

